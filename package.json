{
	"name": "mathematic-literate",
	"displayName": "Literate",
	"publisher": "mathematic",
	"repository": {
		"type": "git",
		"url": "https://github.com/mathematic-inc/vscode-literate.git"
	},
	"description": "Generate literate documentation for your code",
	"version": "0.4.0",
	"icon": "public/icon.png",
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"files": [
		"out",
		"public",
		"LICENSE",
		"README.md"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "literate.generateDocs",
				"title": "Generate Literate Documentation",
				"category": "Literate"
			},
			{
				"command": "literate.resetApiKey",
				"title": "Reset OpenAI API Key",
				"category": "Literate"
			},
			{
				"command": "literate.changeSkillLevel",
				"title": "Change Documentation Detail Level",
				"category": "Literate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "make build",
		"test": "vscode-test",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/mocha": "^10.0.10",
		"@types/node": "~24.3.1",
		"@types/vscode": "1.88.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "^3.6.0",
		"rolldown": "1.0.0-beta.35",
		"rollup-plugin-esbuild": "^6.2.1",
		"rollup-plugin-string-import": "^1.2.6",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@vscode/wasm-component-model": "^1.0.2",
		"openai": "^5.19.1",
		"zod": "^4.1.5"
	},
	"overrides": {
		"zod": "^4.1.5"
	}
}
